<h1>User Show Page</h1>
<h3><%= @user.name %></h3>
<ul>
  <li>Mood: <%= @user.mood unless @user.admin %></li>
  <li>Tickets: <%= @user.tickets unless @user.admin %></li>
  <li>Height: <%= @user.height unless @user.admin %></li>
  <% if @user.admin %>
    <li>ADMIN</li>
  <% end %>
</ul>
<%= link_to "See attractions", attractions_path %>
<br>

<%= link_to "Log Out", signout_path, method: "DELETE" %>
